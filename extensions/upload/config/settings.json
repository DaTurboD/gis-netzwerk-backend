{
  "provider": "local-resize",
    "providerOptions": {
      "optimizeOptions": {
        "jpeg": {
          "quality": 80,
          "progressive": true,
        },
        "png": {
          "quality": 80,
          "progressive": true,
        },
        "webp": {
          "alphaQuality": 80,
        },
        "tiff": {}
      },
      "imageSizes": [
        {
          "name": "logo",
          "resizeOptions": {
            "width": 50,
            "height": 50,
            "fit": "contain",
          },
          "isGenerateWebp": true,
        },
        {
          "name": "small",
          "resizeOptions": {
            "width": 380,
            "height": 200,
          },
          "isGenerateWebp": true,
        },
        {
          "name": "medium",
          "resizeOptions": {
            "width": 550,
            "height": 200,
          },
          "isGenerateWebp": true,
        },
        {
          "name": "large",
          "resizeOptions": {
            "width": 1920,
            "height": 400,
          },
          "isGenerateWebp": true,
        },
      ],
    },
  }
}
